            "post": {
                "description": "Actions triggerd the reactions and call the trigger route",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Asana trigger"
                ],
                "summary": "Trigger an Area",
